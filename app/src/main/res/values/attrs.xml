<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SVGImageView">

        <!-- Location of the SVG document. -->
        <attr name="svg" format="reference|string" />
    </declare-styleable>
    <declare-styleable name="SVGDrawingView">
        <attr name="strokeWidth" format="float" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="phase" format="float" />
        <attr name="duration" format="integer" />
        <attr name="fadeFactor" format="float" />
        <attr name="waitRadius" format="dimension" />
        <attr name="arrowLength" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="StateView">
        <attr name="strokeWidth" />
        <attr name="strokeColor" />
        <attr name="phase" />
        <attr name="duration" />
        <attr name="fadeFactor" />
    </declare-styleable>
    <declare-styleable name="SpotlightView">
        <attr name="target" format="reference" />
        <attr name="mask" format="reference" />
    </declare-styleable>

</resources>